{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/realai-elite", "destination": "/content/maistermind/RealAi-Elite/index.html" },
    { "source": "/realai-dashboard", "destination": "/content/maistermind/RealAi-Elite/app.html" }
  ],
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}
